<table class="links-table">
    <thead>
    <tr>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Отчество</th>
        <th>Отдел</th>
        <th>Должность</th>
        <th>Статус</th>
    </tr>
    </thead>
    <tbody>
    {% for employeePosition in employeePositions %}
        <tr>
            <td><a class="table-cell-link" href="{{ path('app_employee_show', {id: employeePosition.employee.id}) }}">{{ employeePosition.employee.secondName }}</a></td>
            <td><a class="table-cell-link" href="{{ path('app_employee_show', {id: employeePosition.employee.id}) }}">{{ employeePosition.employee.firstName }}</a></td>
            <td><a class="table-cell-link" href="{{ path('app_employee_show', {id: employeePosition.employee.id}) }}">{{ employeePosition.employee.patronymic }}</a></td>
            <td><a class="table-cell-link" href="{{ path('app_employee_show', {id: employeePosition.employee.id}) }}">{{ employeePosition.department.name }}</a></td>
            <td><a class="table-cell-link" href="{{ path('app_employee_show', {id: employeePosition.employee.id}) }}">{{ employeePosition.jobTitle.name }}</a></td>
            <td><a class="table-cell-link" href="{{ path('app_employee_show', {id: employeePosition.employee.id}) }}">{{ employeePosition.workingStatus.name }}</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>
